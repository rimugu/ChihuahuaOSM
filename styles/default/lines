		[0x16 road_class=0 road_speed=0 resolution 22-24]
	surface!=* {set tmp:stopMopUp=yes ; add mkgmap:unpaved=1 ; name '${name} (unkn) [pede]' | '(unkn) [pede]' ; addlabel '${access} ${incline}' | '${access}' | '${incline}'} 
