		[0x06 road_class=1 road_speed=3 resolution 20-24]
	surface=* {name '${name} (pav) [minor]' | '(pav) [minor]' ; addlabel '${maxspeed} ${lanes}' | '${maxspeed}' | '${lanes}'} 
		[0x06 road_class=1 road_speed=3 resolution 20-24]
end 
